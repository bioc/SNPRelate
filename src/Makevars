# additional preprocessor options
PKG_CPPFLAGS = -DUSING_R -I.

# to set flags for the linker
PKG_LIBS = -lpthread $(LAPACK_LIBS) $(BLAS_LIBS) $(FLIBS)

all: $(SHLIB)

clean:
	rm -f ../.travis.yml
	rm -f ../.gitignore
